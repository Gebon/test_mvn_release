<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.github.Gebon</groupId>
  <artifactId>test_mvn_release</artifactId>
  <version>1.8</version>
  <name>maven-release-with-jipack</name>
  <url>http://maven.apache.org</url>
  <scm>
    <connection>scm:git:git://git@github.com:Gebon/test_mvn_release.git</connection>
    <developerConnection>scm:git:git@github.com:Gebon/test_mvn_release.git</developerConnection>
    <url>https://github.com/d0k1/jsflight</url>
  </scm>
  <properties>
    <java.version>1.8</java.version>
    <maven-compiler-plugin.version>2.5.1</maven-compiler-plugin.version>
    <maven-release-plugin.version>2.3.2</maven-release-plugin.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <maven-surefire-plugin.version>2.12.4</maven-surefire-plugin.version>
  </properties>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <repositories>
    <repository>
      <id>repo.pentaho.org</id>
      <url>http://mvn.naumen.ru/content/repositories/repo.pentaho.org</url>
    </repository>
    <repository>
      <id>repo.pentaho.org-third-party</id>
      <url>http://mvn.naumen.ru/content/repositories/repo.pentaho.org-third-party</url>
    </repository>
    <repository>
      <id>naumen-releases</id>
      <name>naumen-releases</name>
      <url>http://mvn.naumen.ru/content/repositories/releases/</url>
    </repository>
    <repository>
      <id>naumen-snapshots</id>
      <url>http://mvn.naumen.ru/content/repositories/snapshots/</url>
    </repository>
    <repository>
      <id>spring-libs-release-s3-cache</id>
      <name>spring libs-release-s3-cache</name>
      <url>http://mvn.naumen.ru/content/repositories/spring-libs-release-s3-cache/</url>
    </repository>
    <repository>
      <id>public-jboss</id>
      <url>http://mvn.naumen.ru/content/repositories/public-jboss/</url>
    </repository>
    <repository>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
      <id>google-maven-snapshot-repository</id>
      <name>Google Maven Snapshot Repository</name>
      <url>http://mvn.naumen.ru/content/repositories/google-snapshots/</url>
    </repository>
    <repository>
      <id>spring-milestone</id>
      <name>Spring Portfolio Milestone Repository</name>
      <url>http://mvn.naumen.ru/content/repositories/spring-milestone/</url>
    </repository>
    <repository>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
      <id>codehaus-snapshots</id>
      <name>codehaus-snapshots</name>
      <url>http://mvn.naumen.ru/content/repositories/codehaus-snapshots/</url>
    </repository>
    <repository>
      <id>jitpack.io</id>
      <url>https://jitpack.io</url>
    </repository>
    <repository>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>central</id>
      <name>Central Repository</name>
      <url>https://repo.maven.apache.org/maven2</url>
    </repository>
  </repositories>
  <pluginRepositories>
    <pluginRepository>
      <id>naumen-releases</id>
      <url>http://mvn.naumen.ru/content/repositories/releases/</url>
    </pluginRepository>
    <pluginRepository>
      <id>naumen-snapshots</id>
      <url>http://mvn.naumen.ru/content/repositories/snapshots/</url>
    </pluginRepository>
    <pluginRepository>
      <id>central-mirror</id>
      <url>http://mvn.naumen.ru/content/repositories/central/</url>
    </pluginRepository>
    <pluginRepository>
      <id>public-jboss</id>
      <url>http://mvn.naumen.ru/content/repositories/public-jboss/</url>
    </pluginRepository>
    <pluginRepository>
      <releases>
        <enabled>true</enabled>
        <updatePolicy>never</updatePolicy>
      </releases>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
      <id>Naumen Codehaus Plugin Repository</id>
      <name>Naumen Codehaus Plugin Repository</name>
      <url>http://mvn.naumen.ru/content/repositories/codehaus-snapshots/</url>
    </pluginRepository>
    <pluginRepository>
      <releases>
        <updatePolicy>never</updatePolicy>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>central</id>
      <name>Central Repository</name>
      <url>https://repo.maven.apache.org/maven2</url>
    </pluginRepository>
  </pluginRepositories>
  <build>
    <sourceDirectory>src/main/java</sourceDirectory>
    <scriptSourceDirectory>/home/bigbear/test_mvn_release/src/main/scripts</scriptSourceDirectory>
    <testSourceDirectory>src/test/java</testSourceDirectory>
    <outputDirectory>target/classes</outputDirectory>
    <testOutputDirectory>target/test-classes</testOutputDirectory>
    <resources>
      <resource>
        <directory>src/main/resources</directory>
      </resource>
    </resources>
    <testResources>
      <testResource>
        <directory>src/test/resources</directory>
      </testResource>
    </testResources>
    <directory>target</directory>
    <plugins>
      <plugin>
        <artifactId>maven-release-plugin</artifactId>
        <version>2.3.2</version>
        <extensions>false</extensions>
      </plugin>
    </plugins>
  </build>
  <reporting>
    <outputDirectory>target/site</outputDirectory>
  </reporting>
</project>
